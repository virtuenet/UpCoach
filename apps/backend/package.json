{
  "name": "@upcoach/backend",
  "version": "1.0.0",
  "private": true,
  "description": "UpCoach Backend API Service",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:reset": "node scripts/reset.js",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@upcoach/shared": "workspace:*",
    "@upcoach/types": "workspace:*",
    "@upcoach/utils": "workspace:*",
    "express": "^4.18.2",
    "sequelize": "^6.35.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "stripe": "^14.5.0",
    "openai": "^4.20.0",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "nodemailer": "^6.9.7",
    "i18next": "^23.7.6",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "axios-retry": "^3.8.0",
    "opossum": "^8.1.0",
    "bull": "^4.11.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16"
  }
}