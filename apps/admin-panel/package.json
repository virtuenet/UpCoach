{
  "name": "@upcoach/admin-panel",
  "version": "1.0.0",
  "private": true,
  "description": "UpCoach Admin Panel",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:a11y": "jest --testMatch='**/*.a11y.test.{js,ts,tsx}'",
    "test:a11y:ci": "jest --testMatch='**/*.a11y.test.{js,ts,tsx}' --ci --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:a11y": "eslint src --ext .ts,.tsx --config .eslintrc.a11y.js",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@tanstack/react-query": "^5.22.2",
    "@tanstack/react-query-devtools": "^5.22.2",
    "@upcoach/shared": "file:../../packages/shared",
    "@upcoach/types": "file:../../packages/types",
    "@upcoach/ui": "file:../../packages/ui",
    "@upcoach/utils": "file:../../packages/utils",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "i18next": "^23.7.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.20.0",
    "recharts": "^2.10.3",
    "tailwind-merge": "^2.1.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@axe-core/react": "^4.8.4",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.6.1",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.8.4",
    "axe-playwright": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
