# Staging Environment Configuration

# Node Environment
NODE_ENV=staging
PORT=8080

# Database
DATABASE_URL=postgresql://upcoach:upcoach_staging@postgres:5432/upcoach_staging
DB_USER=upcoach
DB_PASSWORD=upcoach_staging
DB_NAME=upcoach_staging

# Redis
REDIS_URL=redis://:upcoach_redis_staging@redis:6379
REDIS_PASSWORD=upcoach_redis_staging

# JWT Secret
JWT_SECRET=your-staging-jwt-secret-key-change-this

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key

# OpenAI
OPENAI_API_KEY=your-openai-api-key

# Claude (Anthropic)
CLAUDE_API_KEY=your-claude-api-key

# Stripe
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret
STRIPE_PUBLISHABLE_KEY=pk_test_your-publishable-key

# Email Configuration
EMAIL_FROM=noreply@staging.upcoach.ai
SMTP_HOST=smtp.sendgrid.net
SMTP_PORT=587
SMTP_USER=apikey
SMTP_PASS=your-sendgrid-api-key

# Frontend URLs
FRONTEND_URL=https://staging.upcoach.ai
ADMIN_URL=https://admin-staging.upcoach.ai
VITE_API_URL=https://api-staging.upcoach.ai
NEXT_PUBLIC_API_URL=https://api-staging.upcoach.ai

# Google Analytics
GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Monitoring
GRAFANA_USER=admin
GRAFANA_PASSWORD=admin_staging_password

# Logging
LOG_LEVEL=info

# File Upload
MAX_FILE_SIZE=10485760
UPLOAD_DIR=/app/uploads

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# CORS
CORS_ORIGINS=https://staging.upcoach.ai,https://admin-staging.upcoach.ai,http://localhost:3000,http://localhost:8006

# Security
BCRYPT_ROUNDS=10
SESSION_SECRET=your-session-secret-change-this

# Feature Flags
ENABLE_AI_COACH=true
ENABLE_VOICE_AI=true
ENABLE_PREDICTIONS=true
ENABLE_CMS=true

# AI Service Limits
AI_RATE_LIMIT_PER_USER=50
AI_TOKEN_LIMIT_PER_REQUEST=2000
AI_MONTHLY_TOKEN_LIMIT=1000000