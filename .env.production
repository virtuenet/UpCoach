# UpCoach Production Environment - Port Range 1000-1999
NODE_ENV=production

# Database Configuration (Docker container on port 1433)
DATABASE_URL=postgresql://postgres:postgres@localhost:1433/upcoach_db
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=upcoach_db

# Redis Configuration (Docker container on port 1003)
REDIS_URL=redis://localhost:1003

# JWT Configuration
JWT_SECRET=super-secure-jwt-secret-key-at-least-32-characters-long-production-upcoach-2024
JWT_EXPIRES_IN=7d
JWT_REFRESH_SECRET=super-secure-jwt-refresh-secret-key-at-least-32-characters-long-production-upcoach-2024
JWT_REFRESH_EXPIRES_IN=30d

# API Configuration (Backend on port 1080)
API_URL=http://localhost:1080
PORT=1080

# Frontend URLs
ADMIN_PANEL_URL=http://localhost:1006
LANDING_PAGE_URL=http://localhost:1005
CMS_PANEL_URL=http://localhost:1007

# Site Configuration
SITE_URL=http://localhost:1005
ALLOWED_URLS=http://localhost:*

# Minimal required for basic functionality
ENABLE_BASIC_AUTH=true
ENABLE_HEALTH_CHECKS=true

# Development placeholders (can be updated later)
OPENAI_API_KEY=placeholder
CLAUDE_API_KEY=placeholder
STRIPE_SECRET_KEY=placeholder
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=placeholder
CLERK_SECRET_KEY=placeholder

# Disable optional features for minimal deployment
ENABLE_CLAUDE=false
ENABLE_VOICE_AI=false
ENABLE_PREDICTIVE_ANALYTICS=false
ENABLE_ADAPTIVE_LEARNING=false
SENTRY_DSN=
GOOGLE_ANALYTICS_ID=
MIXPANEL_TOKEN=
