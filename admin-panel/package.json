{
  "name": "upcoach-admin-panel",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "UpCoach Admin Panel - User and platform management dashboard",
  "keywords": ["react", "vite", "admin-panel", "dashboard", "upcoach"],
  "author": "UpCoach Team",
  "license": "Proprietary",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
