    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/natural": "^5.1.5",
    "@types/node": "^20.19.13",
    "@types/opossum": "^8.1.9",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
