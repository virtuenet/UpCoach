{
  "name": "@upcoach/api",
  "version": "1.0.0",
  "description": "UpCoach Backend API Service",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "prebuild": "npm run lint:fix || true",
    "custom-build": "node scripts/custom-build.js",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' npm run custom-build",
    "test": "jest",
    "test:ai": "jest --config jest.ai-services.config.js",
    "test:ai:unit": "jest --config jest.ai-services.config.js --selectProjects=\"AI Services - Unit Tests\"",
    "test:ai:integration": "jest --config jest.ai-services.config.js --selectProjects=\"AI Services - Integration Tests\"",
    "test:ai:performance": "jest --config jest.ai-services.config.js --selectProjects=\"AI Services - Performance Tests\"",
    "test:ai:contracts": "jest --config jest.ai-services.config.js --selectProjects=\"AI Services - Contract Tests\"",
    "test:ai:scenarios": "jest --config jest.ai-services.config.js --selectProjects=\"AI Services - Scenario Tests\"",
    "test:ai:coverage": "jest --config jest.ai-services.config.js --coverage",
    "test:ai:watch": "jest --config jest.ai-services.config.js --watch",
    "test:ai:ci": "jest --config jest.ai-services.config.js --coverage --ci --watchAll=false",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts --max-warnings 1000",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@node-saml/node-saml": "^5.1.0",
    "@sentry/node": "^10.8.0",
    "@sentry/profiling-node": "^10.8.0",
    "@simplewebauthn/server": "^9.0.3",
    "@simplewebauthn/types": "^12.0.0",
    "@supabase/supabase-js": "^2.38.0",
    "@types/date-fns": "^2.5.3",
    "@types/handlebars": "^4.0.40",
    "@types/ioredis": "^4.28.10",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/speakeasy": "^2.0.10",
    "@types/stripe": "^8.0.416",
    "@types/uuid": "^10.0.0",
    "async-mutex": "^0.5.0",
    "axios": "^1.6.2",
    "axios-retry": "^4.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dd-trace": "^5.64.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "file-type": "^21.0.0",
    "google-auth-library": "^9.15.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "i18n": "^0.15.1",
    "ioredis": "^5.6.1",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^10.4.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "natural": "^8.1.0",
    "node-cron": "^4.2.1",
    "node-dogstatsd": "^0.0.7",
    "nodemailer": "^7.0.5",
    "openai": "^4.104.0",
    "openid-client": "^6.6.3",
    "opossum": "^8.5.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.2.2",
    "sentiment": "^5.0.2",
    "sequelize-typescript": "^2.1.6",
    "sharp": "^0.34.3",
    "slugify": "^1.6.6",
    "speakeasy": "^2.0.0",
    "stripe": "^18.3.0",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
