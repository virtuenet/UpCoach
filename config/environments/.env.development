# UpCoach Development Environment
NODE_ENV=development
ENVIRONMENT=development

# Database Configuration (Local development)
DB_HOST=localhost
DB_PORT=5432
DB_USER=upcoach
DB_PASSWORD=dev_password_123
DB_NAME=upcoach_dev
DATABASE_URL=postgresql://upcoach:dev_password_123@localhost:5432/upcoach_dev

# Redis Configuration (Local development)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_URL=redis://localhost:6379

# JWT & Authentication (Development secrets - not for production)
JWT_SECRET=dev_jwt_secret_minimum_32_characters_long
JWT_EXPIRES_IN=24h
REFRESH_TOKEN_SECRET=dev_refresh_token_secret_minimum_32_characters_long
REFRESH_TOKEN_EXPIRES_IN=7d

# Application URLs (Local development)
FRONTEND_URL=http://localhost:3000
ADMIN_URL=http://localhost:3001
API_URL=http://localhost:8080

# Frontend Environment Variables
VITE_API_URL=http://localhost:8080
VITE_ENVIRONMENT=development

# Next.js Environment Variables
NEXT_PUBLIC_API_URL=http://localhost:8080

# Email Configuration (Development - console logging)
EMAIL_FROM=dev@upcoach.ai
SMTP_HOST=localhost
SMTP_PORT=587
SMTP_SECURE=false

# Security (Relaxed for development)
CORS_ORIGIN=http://localhost:3000,http://localhost:3001
LOG_LEVEL=debug
ENABLE_DEBUG_LOGGING=true
ENABLE_QUERY_LOGGING=true

# Feature Flags (All enabled for development)
ENABLE_FEATURE_FLAGS=true
ENABLE_AI_COACHING=true
ENABLE_VOICE_JOURNAL=true
ENABLE_ANALYTICS=true

# Performance (Relaxed for development)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000
CACHE_TTL=60

# Database Sync (Enable alter mode for development)
DB_SYNC_MODE=alter

# Local LLM Configuration (Development paths)
LOCAL_LLM_MODEL_PATH=./models/upcoach-ai.gguf
LOCAL_LLM_MODELS_DIR=./models

# External Services (Use test/development keys)
# OPENAI_API_KEY=your_openai_api_key_here
# CLAUDE_API_KEY=your_claude_api_key_here
# STRIPE_SECRET_KEY=sk_test_your_stripe_test_secret_key_here
# SUPABASE_URL=https://your-dev-project.supabase.co
# SUPABASE_ANON_KEY=your_dev_supabase_anon_key_here