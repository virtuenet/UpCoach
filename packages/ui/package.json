{
  "name": "@upcoach/ui",
  "version": "1.0.0",
  "description": "UI component library for UpCoach",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
